
@{
    ViewData["Title"] = "PlanetResults";
}

@model SWPlanets
<h1>@ViewBag.Message</h1>

<div class="table-bordered table">
    <table>
        <tr>
            <th>Name</th>
            <th>Rotation Period</th>
            <th>Orbital Period</th>
            <th>Diameter</th>
            <th>Climate</th>
            <th>Gravity</th>
            <th>Terrain</th>
            <th>Surface Water</th>
            <th>Population</th>
        </tr>
        <tr>
            <td>@Model.name</td>
            <td>@Model.rotation_period</td>
            <td>@Model.orbital_period</td>
            <td>@Model.diameter</td>
            <td>@Model.climate</td>
            <td>@Model.gravity</td>
            <td>@Model.terrain</td>
            <td>@Model.surface_water</td>
            <td>@Model.population</td>
        </tr>
    </table>
</div>

